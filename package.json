    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/elasticsearch": "^7.1.0",
    "@nestjs/event-emitter": "^0.0.3",
    "@nestjs/graphql": "^7.7.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/swagger": "^4.7.0",
    "@nextnm/nestjs-mailgun": "^1.0.4",
    "@ntegral/nestjs-sentry": "^2.0.6",
    "@sentry/node": "^6.2.2",
    "ali-oss": "^6.13.1",
    "apollo-server-express": "^2.18.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.12.2",
    "ejs": "^3.1.6",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.3",
    "md5": "^2.3.0",
    "mongoose": "^5.13.5",
    "mongoose-autopopulate": "^0.14.0",
    "mongoose-paginate": "^5.0.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^4.5.1",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "request-country": "^0.1.5",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "slate": "^0.72.3",
    "swagger-ui-express": "^4.1.4",
    "ts-retry-promise": "^0.6.0",
    "typescript": "4.6.0-dev.20211228"
